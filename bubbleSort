#include<iostream>
using namespace std;
main()
{
	int i,j,n,a[100],temp=0;
	
	
	cout<<"Enter Value of Array Elements : ";
	cin>>n;
	
	for(i=0; i<n; i++)
	{
		cout<<"a["<<i<<"] : ";
		cin>>a[i];
	}
	
	for(i=0; i<n; i++)
	{
		for(j=i+1; j<n; j++)
		{
			if(a[i] > a[j])
			{
				temp = a[i];
				a[i] = a[j];
				a[j] = temp;
			}
		}
	}
	
	cout<<"\n\nAfter Sorting \n\n";
	
	for(i=0; i<n; i++)
	{
		cout<<a[i]<<" ";
	}
	
}
